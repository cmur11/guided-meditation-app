.DS_Store


# Ignore bundler config.
meditation-app-backend/.bundle

# Ignore the default SQLite database.
meditation-app-backend/db/*.sqlite3
meditation-app-backend/db/*.sqlite3-journal
meditation-app-backend/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
meditation-app-backend/log/*
meditation-app-backend/tmp/*
meditation-app-backend/!/log/.keep
meditation-app-backend/!/tmp/.keep
meditation-app-backend/.byebug_history

# Ignore uploaded files in development.
meditation-app-backend/storage/*
meditation-app-backend/!/storage/.keep
meditation-app-backend/.byebug_history

# Ignore master key for decrypting credentials and more.
meditation-app-backend/config/master.key